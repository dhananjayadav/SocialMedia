<%- include('partials/navbar') %>
<div class="postCreateBox">
    <form class="createBox" action="create-post" method="POST" enctype="multipart/form-data">
        <input id="postcreatetitle" class="contentpost" type="text" name="title" placeholder="Enter title here">
        <textarea id="postcreatecontent" class="contentpost" name="content" id="" cols="30" rows="5" placeholder="Enter about your post here..."></textarea>
        <input id="postfile" type="file" name="avatar">
        <button  id="postbtn" class="postBtn">Post</button>
        <button hidden id="submitPost"></button>
    </form>
</div>
<script src="/javascripts/post.js"></script>
